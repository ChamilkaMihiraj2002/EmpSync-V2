generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id String @id
  name String @unique
  role String 
  dob DateTime
  telephone String
  address String
  email String @unique
  password String
  thumbId Bytes @unique
  createdAt DateTime @default(now())
}

